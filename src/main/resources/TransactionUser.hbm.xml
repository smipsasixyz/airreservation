<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name="com.bsr.air.domain.TransactionUser" table="Transact">
      <meta attribute="class-description">
         This class contains the employee detail. 
      </meta>
      <id name="transid" type="int">
         <generator class="native"/>
      </id>
      <property name="uid" column="upid"  type="int"/>
      <property name="transamount" column="trans_amount" type="double"/>
      <property name="transdate" column="trans_date"  type="date"/>
      <property name="transmsg" column="trans_status"  type="string"/>
      </class>
</hibernate-mapping>